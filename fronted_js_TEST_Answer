6.) a.) const names=people.map(person+>person.name);
        console.log(names);
    b.) const peopleWithSalary=people.map(person=>({...person,salary:50000}));
        console.log(peopleWithSalary);
    c.) const aboveThirty=people
        .filter(person => person.age>30) 
        .map(person => ({ name:person.name,age:person,age}));
        console.log(aboveThirty);
7.) const cb1=(a,b) => a+b;
    const cb2=(a,b) => a-b;
    function main(cb1, cb2, x, y){
        const sum = cb1(x, y);
        const diff=cb2(x, y);

        console.log(`${sum}`);
        console.log(`${diff}`);
    }
    const x=11;
    const y=4;

    main(cb1, cb2, x, y);
